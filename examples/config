	// This is a sample configuration used by examples programs

	SECTION_ADD Sa Sb

SECTION Sa

	// resources for example program 1-simple

	LET_ENUM enum 5
	LET ratio 0.5

	example-1 a TIME
	example-1 b 45.5
	example-1 c 50 60 70
	example-1 d TRUE
	example-1 e new_value
	example-1 f ($$ example_param)
	example-1 g (CITRPL #000000 #ffff8000 ($ ratio))
	example-1 h ($ enum)

SECTION Sb

	// resources for example program 3-threads

	SEED ($$ sim_id)

	example-2 coordinates (RAND 10 90) (RAND 10 90) (RAND 10 90)

	LET vals aB cD eF

	FOR_EACH vals x
		DEBUG_PRINT test1 a b c % x
		DEBUG_PRINT test2 0 1 2 % x
		DEBUG_PRINT test3 9 8 7 % x
		DEBUG_PRINT test4 x y z % x
	FOR_END

	FOR_EACH enum x
		LET y (* (% x) 10)
		DEBUG_PRINT test1 a b c ($ y)
		DEBUG_PRINT test2 0 1 2 ($ y)
		DEBUG_PRINT test3 9 8 7 ($ y)
		DEBUG_PRINT test4 x y z ($ y)	
	FOR_END
