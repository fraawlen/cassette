	// This is a sample configuration used by examples programs

	SECTION_ADD Sa Sb

SECTION Sa

	// resources for example program 1-simple

	LET_ENUM enum 5
	LET ratio 0.5

	example-1 a TIME
	example-1 b 45.5
	example-1 c 50 60 70
	example-1 d TRUE
	example-1 e new_value
	example-1 f ($$ example_param)
	example-1 g (CITRPL #000000 #ffff8000 ($ ratio))
	example-1 h ($ enum)

SECTION Sb

	// resources for example program 3-threads

	SEED ($$ sim_id)

	example-2 coordinates (RAND 10 90) (RAND 10 90) (RAND 10 90)
